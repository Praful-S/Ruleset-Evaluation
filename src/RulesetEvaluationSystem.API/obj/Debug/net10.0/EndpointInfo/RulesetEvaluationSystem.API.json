{
  "openapi": "3.0.4",
  "info": {
    "title": "RulesetEvaluationSystem.API",
    "version": "1.0"
  },
  "paths": {
    "/api/Evaluation/evaluate": {
      "post": {
        "tags": [
          "Evaluation"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/OrderDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/OrderDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/EvaluationResultDto"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/api/Evaluation/health": {
      "get": {
        "tags": [
          "Evaluation"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Logs/order/{orderId}": {
      "get": {
        "tags": [
          "Logs"
        ],
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Logs/recent": {
      "get": {
        "tags": [
          "Logs"
        ],
        "parameters": [
          {
            "name": "count",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32",
              "default": 50
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Rulesets": {
      "get": {
        "tags": [
          "Rulesets"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Rulesets/{id}": {
      "get": {
        "tags": [
          "Rulesets"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ComponentDto": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "nullable": true
          },
          "attributes": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EvaluationResultDto": {
        "type": "object",
        "properties": {
          "matched": {
            "type": "boolean"
          },
          "productionPlant": {
            "type": "string",
            "nullable": true
          },
          "matchedRuleset": {
            "type": "string",
            "nullable": true
          },
          "matchedRule": {
            "type": "string",
            "nullable": true
          },
          "reason": {
            "type": "string",
            "nullable": true
          },
          "evaluatedAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "ItemDto": {
        "type": "object",
        "properties": {
          "sku": {
            "type": "string",
            "nullable": true
          },
          "printQuantity": {
            "type": "integer",
            "format": "int32"
          },
          "components": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ComponentDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "OrderDto": {
        "type": "object",
        "properties": {
          "orderId": {
            "type": "string",
            "nullable": true
          },
          "publisherNumber": {
            "type": "string",
            "nullable": true
          },
          "publisherName": {
            "type": "string",
            "nullable": true
          },
          "orderMethod": {
            "type": "string",
            "nullable": true
          },
          "shipments": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ShipmentDto"
            },
            "nullable": true
          },
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ItemDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "ShipToDto": {
        "type": "object",
        "properties": {
          "isoCountry": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ShipmentDto": {
        "type": "object",
        "properties": {
          "shipTo": {
            "$ref": "#/components/schemas/ShipToDto"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "tags": [
    {
      "name": "Evaluation"
    },
    {
      "name": "Logs"
    },
    {
      "name": "Rulesets"
    }
  ]
}